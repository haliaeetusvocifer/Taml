# Comments test
# Tests that comments are properly ignored

# Top-level comment
application	TestApp

# Comment between key-value pairs
version	1.0.0
# This is a comment
author	Developer

# Comments in nested structures
server
	# Server configuration
	host	0.0.0.0
	port	3000
	# SSL settings below
	ssl
		enabled	true
		# Certificate path
		cert	/etc/ssl/cert.pem

# Comment with special characters and symbols: spaces  symbols!@#$%
database
	type	postgres
	# Connection details
	host	db.server.com
